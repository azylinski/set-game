{
  "name": "set-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "@types/jest": "24.0.23",
    "@types/js-combinatorics": "^0.5.31",
    "@types/node": "12.12.12",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "boardgame.io": "^0.34.1",
    "js-combinatorics": "^0.5.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "typescript": "3.7.2"
  },
  "typeRoots": [
    "node_modules/@types",
    "@types"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "svg-tsc": "./node_modules/typescript/bin/tsc --esModuleInterop -t ES5 --downlevelIteration src/utils/generate-svg-icons.ts",
    "svg-build": "node src/utils/generate-svg-icons.js",
    "svg-cleanup": "rm src/utils/generate-svg-icons.js src/core/model.js",
    "generateSvgIcons": "npm run -s svg-tsc && npm run -s svg-build && npm run -s svg-cleanup"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/pug": "^2.0.4",
    "pug": "^2.0.4"
  }
}
